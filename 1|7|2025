lines = int(input("How many lines did you clear?: "))
if lines < 100:
    print("Novice")
elif lines < 200:
    print("Intermediate")
elif lines < 300:
    print("Advanced")
else:
    print("Expert")

for i in range(2, 80, 10):
    print(i, end = " ")

for i in range(100, 10, -5):
    print(i, end = " ")
